@model Dictionary<string,string>

@{
    ViewData["Title"] = "Index";
}

<div class="container">
    <div class="row">
        <div class="col-lg-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">HomeSlider table</h4>
                    <a asp-action="create" class="btn btn-outline-success">Create HomeSlider</a>

                    <div class="table-responsive">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>Key</th>
                                    <th>Value</th>
                                    <th>CreatedAt</th>
                                    <th>AddedBy</th>
                                    <th>UpdateAt</th>
                                    <th>DeleteAt</th>
                                    <th>Settings</th>


                                </tr>
                            </thead>
                            <tbody>

                                @foreach (var KeyAndValue in Model)
                                {
                                    <tr>


                                        <td>@KeyAndValue.Key</td>
                                        <td>@KeyAndValue.Value</td>
                                     


                                        <td>

                                            <a asp-action="update" asp-route-id="@KeyAndValue.Key" class="btn btn-outline-warning">
                                                <i class="mdi mdi-table-edit"></i>
                                            </a>
                                        </td>
                                    </tr>

                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>